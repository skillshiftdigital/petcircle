    "name": "{plpBrandName}", // "blackhawk Pet Supplies"
    "description": "{plpBrandDescription}", // "Top-quality products by blackhawk for your pets."
    "url": "{plpBrandUrl}", // "https://www.petcircle.com.au/brands/blackhawk"
    "numberOfItems": "{numberOfItems}", // "100" - The total number of blackhawk brand products listed.
    "itemListOrder": "http://schema.org/ItemListOrderAscending", // The order items are listed in, e.g., ascending or descending.
    "brand": {
      "@type": "Brand",
      "name": "{brand.name}", // "blackhawk"
      "logo": "{brand.logoUrl}" // "https://www.petcircle.com.au/logos/blackhawk.png"
    },
    "itemListElement": [
      {
        "@type": "ListItem",
        "position": 1,
        "item": {
